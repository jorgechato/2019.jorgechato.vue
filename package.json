{
  "name": "jorgechato.com",
  "version": "1.0.0",
  "description": "Personal web page [frontend]",
  "author": "jorgechato",
  "private": true,
  "main": "nuxt.config.js",
  "scripts": {
    "start": "npm run build:dev",
    "build": "cross-env NODE_ENV=production nuxt generate --spa",
    "build:dev": "nuxt",
    "serve": "nuxt build && nuxt start",
    "serve:static": "http-server ./dist/ -p 3000 -c 31536000 --gzip",
    "lint": "npm run lint:js && npm run lint:vue",
    "lint:js": "eslint *.js config",
    "lint:vue": "eslint --ext .vue,.js src",
    "test": "npm run test:unit",
    "test:unit": "jest -c jest.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.2",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-i18n": "^6.13.9",
    "rxjs": "^6.6.2",
    "showdown": "^1.9.1",
    "vue-breakpoints": "^1.1.0",
    "vue-i18n": "^8.21.0",
    "vue-lazyload": "^1.3.3",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^4.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "glob-all": "^3.2.1",
    "jest": "^26.4.0",
    "node-sass": "^4.14.1",
    "purgecss-webpack-plugin": "^2.3.0",
    "sass-loader": "^9.0.3"
  }
}
